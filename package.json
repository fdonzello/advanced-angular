{
  "name": "advanced",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "app:setup": "npm i",
    "app:serve": "ng serve",
    "app:build": "ng build --prod",
    "app:server": "node_modules/.bin/json-server server/db.json",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "ng build --prod && ng run advanced:server:production",
    "app:universal:build": "npm run build:ssr",
    "app:universal:serve": "npm run serve:ssr"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
